<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ionic.starter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Safebags</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework.com" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="https://*.google.com/* " />
    <preference name="ScrollEnabled" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^5.0.0" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <plugin name="phonegap-plugin-barcodescanner" spec="8.1.0" />
    <plugin name="cordova-plugin-ble-central" spec="1.7.2" />
    <plugin name="cordova-plugin-android-permissions" spec="^1.1.5" />
    <platform name="android">
        <preference name="Orientation" value="portrait" />
        <preference name="AndroidWindowSplashScreenAnimatedIcon" value="resources/android/splash/5908863.png" />
        <preference name="AndroidSplashDrawableLandLdpi" value="resources/android/splash/drawable-land-ldpi-screen.png" />
        <preference name="AndroidSplashDrawableLandMdpi" value="resources/android/splash/drawable-land-mdpi-screen.png" />
        <preference name="AndroidSplashDrawableLandHdpi" value="resources/android/splash/drawable-land-hdpi-screen.png" />
        <preference name="AndroidSplashDrawableLandXhdpi" value="resources/android/splash/drawable-land-xhdpi-screen.png" />
        <preference name="AndroidSplashDrawableLandXxhdpi" value="resources/android/splash/drawable-land-xxhdpi-screen.png" />
        <preference name="AndroidSplashDrawableLandXxxhdpi" value="resources/android/splash/drawable-land-xxxhdpi-screen.png" />
        <preference name="AndroidSplashDrawablePortLdpi" value="resources/android/splash/drawable-port-ldpi-screen.png" />
        <preference name="AndroidSplashDrawablePortMdpi" value="resources/android/splash/drawable-port-mdpi-screen.png" />
        <preference name="AndroidSplashDrawablePortHdpi" value="resources/android/splash/drawable-port-hdpi-screen.png" />
        <preference name="AndroidSplashDrawablePortXhdpi" value="resources/android/splash/drawable-port-xhdpi-screen.png" />
        <preference name="AndroidSplashDrawablePortXxhdpi" value="resources/android/splash/drawable-port-xxhdpi-screen.png" />
        <preference name="AndroidSplashDrawablePortXxxhdpi" value="resources/android/splash/drawable-port-xxxhdpi-screen.png" />
        <preference name="SplashMaintainAspectRatio" value="true" />
    </platform>
    <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
        <string>We use your location for full functionality of certain app features.</string>
    </edit-config>
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyCuVwgjIa6k3Ju16xCawZkLpA1" />
    <preference name="GOOGLE_MAPS_IOS_API_KEY" value="AIzaSyCuVwgjIa6k3Ju16xCawZkLpA1" />
</widget>
